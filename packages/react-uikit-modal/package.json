{
  "name": "react-uikit-modal",
  "version": "1.0.1",
  "description": "React uikit modal component.",
  "keywords": [
    "javascript",
    "react",
    "uikit",
    "react-component",
    "react-uikit",
    "uikit-react",
    "modal"
  ],
  "repository": "git@github.com:otissv/react-uikit-modal.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "./lib/modal.js",
  "scripts": {
    "build": "babel -d lib/ src/ && date",
    "build:watch": "watch 'npm run build' ./src",
    "lint": "eslint --quiet ./src/**  && date || true  && date",
    "lint:watch": "watch 'npm run lint' ./",
    "prepublish": "babel -d lib/ src/ && date",
    "test": "babel-node test-runner.js | tap-min && date || true && date",
    "test:spec": "babel-node test-runner.js | tap-difflet && date || true  && date",
    "test:watch": "watch 'npm run test' ./"
  },
  "devDependencies": {
    "babel-cli": "~6.8.0",
    "babel-core": "~6.8.0",
    "babel-eslint": "~6.0.4",
    "babel-plugin-transform-runtime": "~6.8.0",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-runtime": "~6.6.1",
    "cheerio": "~0.20.0",
    "eslint": "~2.9.0",
    "eslint-config-standard": "~5.3.0",
    "eslint-loader": "~1.3.0",
    "eslint-plugin-react": "~5.0.1",
    "eslint-plugin-smells": "~1.0.1",
    "eslint-plugin-standard": "~1.3.2",
    "faucet": "~0.0.1",
    "glob": "~7.0.3",
    "history": "~2.1.1",
    "react-addons-test-utils": "~15.0.2",
    "tap-difflet": "~0.4.0",
    "tap-min": "~1.1.0",
    "tape": "~4.5.1",
    "watch": "~0.18.0"
  },
  "dependencies": {
    "react": "~15.0.2",
    "react-uikit-base": "~1.0.0",
    "react-uikit-button": "~1.0.0",
    "react-uikit-trigger": "~1.0.0",
    "ufunc": "~0.2.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-2"
    ]
  },
  "engines": {
    "node": "~4.4.3"
  }
}
